  <entity class="edairy.model.dairy.impl.MembershipImpl" name="Membership">
    <table name="MEMBERSHIP"/>
    <inheritance strategy="JOINED"/>
    <attributes>
      <basic fetch="EAGER" name="applicationDate" optional="false">
        <temporal>TIMESTAMP</temporal>
      </basic>
      <basic fetch="EAGER" name="effectiveDate" optional="true">
        <temporal>TIMESTAMP</temporal>
      </basic>
      <basic fetch="EAGER" name="status" optional="false">
