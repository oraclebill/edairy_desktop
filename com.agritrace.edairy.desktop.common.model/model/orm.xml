        <cascade>
          <cascade-persist/>
          <cascade-merge/>
          <cascade-refresh/>
        </cascade>
      </one-to-many>
      <one-to-many fetch="LAZY" mapped-by="eContainer" name="routes" target-entity="edairy.model.dairy.impl.RouteDefinitionImpl">
        <cascade>
          <cascade-all/>
        </cascade>
      </one-to-many>
      <one-to-many fetch="LAZY" mapped-by="eContainer" name="vehicles" target-entity="edairy.model.dairy.impl.VehicleImpl">
        <cascade>
          <cascade-all/>
        </cascade>
      </one-to-many>
      <one-to-many fetch="LAZY" mapped-by="eContainer" name="employees" target-entity="edairy.model.dairy.impl.EmployeeImpl">
        <cascade>
          <cascade-all/>
        </cascade>
      </one-to-many>
      <one-to-many fetch="LAZY" mapped-by="eContainer" name="sessions" target-entity="edairy.model.dairy.impl.SessionImpl">
        <cascade>
          <cascade-all/>
        </cascade>
      </one-to-many>
      <one-to-many fetch="LAZY" mapped-by="eContainer" name="memberships" target-entity="edairy.model.dairy.impl.MembershipImpl">
        <cascade>
          <cascade-all/>
