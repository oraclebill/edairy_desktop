<plugin>
	<extension point="org.eclipse.ui.perspectives">
		<perspective
			class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
			 id="com.agritrace.edairy.desktop.home"
			name="Home">
		</perspective>
		<perspective
			class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
			 id="com.agritrace.edairy.desktop.milk"
			name="Milk">
		</perspective>
		<perspective
			class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
			 id="com.agritrace.edairy.desktop.members"
			name="Members">
		</perspective>
		<perspective
			class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
			 id="com.agritrace.edairy.desktop.finance"
			name="Finance">
		</perspective>
		<perspective
			class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
			 id="com.agritrace.edairy.desktop.animalhealth"
			name="Veterinary">
		</perspective>
		<perspective
			class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
			 id="com.agritrace.edairy.desktop.operations"
			name="Operations">
		</perspective>
		<perspective
			class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
			 id="com.agritrace.edairy.desktop.reports"
			name="Reports">
		</perspective>
		<perspective
			class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
			 id="com.agritrace.edairy.desktop.system"
			name="System">
		</perspective>
	</extension>
	<extension point="org.eclipse.ui.views">
		<view allowMultiple="true"
			class="com.agritrace.edairy.desktop.common.ui.views.BlankView" id="default.blank.view"
			name="Blank" restorable="true">
		</view>
		<view allowMultiple="true"
			class="com.agritrace.edairy.desktop.ui.views.MilkProductionReportView"
			id="reports.milk.production" name="Milk Production Report"
			restorable="true">
		</view>

		<view allowMultiple="true"
			class="com.agritrace.edairy.desktop.ui.views.MemberPayablesReportView"
			id="com.agritrace.edairy.desktop.ui.views.MemberPayablesReportView" name="Member Payables Report"
			restorable="true">
		</view>
		<view allowMultiple="true"
			class="com.agritrace.edairy.desktop.ui.views.MemberStatementReportView"
			id="com.agritrace.edairy.desktop.ui.views.MemberStatementReportView" name="Member Statement Report"
			restorable="true">
		</view>
	</extension>

   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.eclipse.riena.demo.client.exitCommand"
            name="Exit">
      </command>
      <command
            id="org.eclipse.riena.demo.client.zoomInCommand"
            name="In">
      </command>
      <command
            id="org.eclipse.riena.demo.client.certainViewCommand"
            name="Certain View (Combo)">
      </command>
      <command
            id="org.eclipse.riena.demo.client.zoomOutCommand"
            name="Out">
      </command>
      <command
            id="org.eclipse.riena.demo.client.certainPerspectiveCommand"
            name="Certain Perspective (Playground)">
      </command>
            <command
            id="org.eclipse.riena.demo.client.histBackCommand"
            name="History Backward">
      </command>
      <command
            id="org.eclipse.riena.demo.client.histForCommand"
            name="History Forward">
      </command>
    </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="fileMenu"
               label="File"
               mnemonic="F">
            <menu
                  id="org.eclipse.riena.demo.client.zoomMenu"
                  label="Zoom"
                  mnemonic="Z">
               <command
                     commandId="org.eclipse.riena.demo.client.zoomInCommand"
                     id="org.eclipse.riena.demo.client.zoomInMenuItem"
                     mnemonic="I"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.riena.demo.client.zoomOutCommand"
                     id="org.eclipse.riena.demo.client.zoomOutMenuItem"
                     mnemonic="O"
                     style="push">
               </command>
            </menu>
            <command
                  commandId="org.eclipse.riena.demo.client.exitCommand"
                  id="org.eclipse.riena.demo.client.exitMenuItem"
                  mnemonic="x">
            </command>
         </menu>
         <!--
         <menu
               label="Preferences"
               mnemonic="P">
            <menu
                  label="Look &apos;n&apos; Feel"
                  mnemonic="L">
               <command
                     commandId="org.eclipse.riena.example.client.rienaDefaultLnfCommand"
                     id="org.eclipse.riena.example.client.rienaDefaultLnfMenuItem"
                     mnemonic="D">
               </command>
               <command
                     commandId="org.eclipse.riena.example.client.exampleLnfCommand"
                     id="org.eclipse.riena.example.client.exampleLnfMenuItem"
                     mnemonic="x">
               </command>
            </menu>
         </menu>
         -->
         <menu
               id="contextMenu"
               label="Context menu"
               mnemonic="x">
            <command
                  commandId="org.eclipse.riena.demo.client.certainViewCommand"
                  icon="icons/ledred.png"
                  id="org.eclipse.riena.demo.client.certainViewMenuItem"
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.certainPerspectiveCommand"
                  icon="icons/ledlightgreen.png"
                  id="org.eclipse.riena.demo.client.certainPerspectiveMenuItem"
                  mnemonic="P"
                  style="push">
            </command>
         </menu>
         <menu
               id="navigationMenu"
               label="Navigation menu"
               mnemonic="N">
            <command
                  commandId="org.eclipse.riena.demo.client.histBackCommand"
                  icon="icons/backward.gif"
                  id="org.eclipse.riena.demo.client.histBackMenuItem"
                  mnemonic="B"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.histForCommand"
                  icon="icons/forward.gif"
                  id="org.eclipse.riena.demo.client.histForMenuItem"
                  mnemonic="F"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.eclipse.riena.demo.client.toolbar">
            <command
                  commandId="org.eclipse.riena.demo.client.exitCommand"
                  id="org.eclipse.riena.demo.client.exitToolBarItem"
                  label="Exit"
                  mnemonic="x"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.certainViewCommand"
                  icon="icons/ledred.png"
                  id="org.eclipse.riena.demo.client.certainViewToolBarItem"
                  label="View"
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.certainPerspectiveCommand"
                  icon="icons/ledlightgreen.png"
                  label="Perspective"
                  mnemonic="P"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.histBackCommand"
                  icon="icons/backward.gif"
                  label="History Back"
                  mnemonic="B"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.histForCommand"
                  icon="icons/forward.gif"
                  label="History Forward"
                  mnemonic="F"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

	<extension point="org.eclipse.riena.ui.swt.imagePaths">
		<path path="resources" />
		<path path="icons" />
	</extension>
	<extension id="dairy_manager_app" name="Dairy Manager"
		point="org.eclipse.core.runtime.applications">
		<application cardinality="singleton-global" thread="main"
			visible="true">
			<run class="com.agritrace.edairy.desktop.ui.EDairyManagerApplication">
			</run>
		</application>
	</extension>

	<extension id="dairy_manager_prod" point="org.eclipse.core.runtime.products">
		<product application="com.agritrace.edairy.desktop.dairy_manager_app"
				 name="beta-plugin">
     <property
           name="appName"
           value="beta-plugin">
     </property>
		</product>
	</extension>
 <extension
       id="dairy_manager_app"
       name="eDairy Manager Application"
       point="org.eclipse.core.runtime.applications">
    <application
          cardinality="singleton-global"
          thread="main"
          visible="true">
