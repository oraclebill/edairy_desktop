            <method name="fetch"><![CDATA[if(currentrow > count){
	return (false);
}

var dataLine = data.get(currentrow);

row["Year"] = ""+dataLine.getDate();
row["MilkCollection"] = dataLine.getProductionString();
row["Change"] = dataLine.getChangeString();
currentrow = currentrow + 1;
return (true);]]></method>
