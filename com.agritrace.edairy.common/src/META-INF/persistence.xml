<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="com.agritrace.edairy.common">
		<description>edairy manager basic persistence </description>
		<class>com.agritrace.edairy.common.model.dairy.Vehicle</class>
		<class>com.agritrace.edairy.common.model.dairy.SecurityRole</class>
		<class>com.agritrace.edairy.common.model.dairy.Route</class>
		<class>com.agritrace.edairy.common.model.dairy.Membership</class>
		<class>com.agritrace.edairy.common.model.dairy.Dairy</class>
		<class>com.agritrace.edairy.common.model.dairy.Container</class>
		<class>com.agritrace.edairy.common.model.core.Person</class>
		<class>com.agritrace.edairy.common.model.core.Party</class>
		<class>com.agritrace.edairy.common.model.core.Location</class>
		<class>com.agritrace.edairy.common.model.core.ContactMethod</class>
		<class>com.agritrace.edairy.common.model.dairy.Employee</class>
		<class>com.agritrace.edairy.common.model.dairy.SystemUser</class>
		<properties>
			<property name="eclipselink.ddl-generation" value="drop-and-create-tables"></property>
			<property name="eclipselink.ddl-generation.output-mode" value="sql-script"></property>
			<property name="eclipselink.target-database" value="SQLAnywhere"></property>
			<property name="eclipselink.target-server" value="None"></property>
			<property name="eclipselink.session-name" value="dairy"></property>
		</properties>
	</persistence-unit>
</persistence>
