            <xml-property name="queryText"><![CDATA[select collectiongroup.journaldate, collectionsession.code as session, dairylocation.code as center, collectionjournalline.quantity, collectionjournalline.rejected
from collectiongroup 
	join collectionjournalline on collectiongroup.journalid = collectionjournalline.collectionjournalline_group_e_id
	join collectionsession on collectionsession.id = collectiongroup.collectionsession_session_e_id
	join dairylocation on dairylocation.id = collectiongroup.dairylocation_collectioncenter_id
where journaldate = ?]]></xml-property>
