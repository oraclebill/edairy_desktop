<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
            icon="icons/edairymanagericon16.png"
            id="com.agritrace.edairy.manager.perspective"
            name="com.agritrace.edairy.manager.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
            icon="icons/edairymanagericon16.png"
            id="com.agritrace.edairy.manager.reports"
            name="com.agritrace.edairy.manager.reports">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
            icon="icons/edairymanagericon16.png"
            id="com.agritrace.edairy.manager.administration"
            name="com.agritrace.edairy.manager.administration">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
            icon="icons/edairymanagericon16.png"
            id="com.agritrace.edairy.manager.main"
            name="com.agritrace.edairy.manager.main">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Dairy Info"
            allowMultiple="true"
            icon="icons/edairymanagericon16.png"
            class="com.agritrace.edairy.dairy.ui.views.DairyProfileView"
            id="com.agritrace.edairy.dairy.ui.views.DairyProfileView">
         <description>
            description body text
         </description>
      </view>
      <view
            name="Dairy Info"
            allowMultiple="true"
            icon="icons/edairymanagericon16.png"
            class="com.agritrace.edairy.dairy.ui.views.DairyLocationView"
            id="com.agritrace.edairy.dairy.ui.views.DairyLocationView">
         <description>
            description body text
         </description>
      </view>
      <view
            name="Demo Dairy Home"
            allowMultiple="true"
            icon="icons/edairymanagericon16.png"
            class="com.agritrace.edairy.ui.views.DairyHomeView"
            id="com.agritrace.edairy.ui.views.DairyHomeView">
      </view>
      <view
            allowMultiple="true"
            class="com.agritrace.edairy.dairy.ui.views.StaffInfoView"
            id="com.agritrace.edairy.dairy.ui.views.StaffInfoView"
            name="StaffInfoView">
      </view>
      <view
            allowMultiple="true"
            class="com.agritrace.edairy.ui.views.VeterinaryRequestView"
            id="com.agritrace.edairy.ui.views.VeterinaryRequestView"
            name="VeterinaryRequestView">
      </view>
      <view
            allowMultiple="true"
            class="com.agritrace.edairy.ui.views.InseminationRequestView"
            id="com.agritrace.edairy.ui.views.InseminationRequestView"
            name="InseminationRequestView">
      </view>
      <view
            allowMultiple="true"
            class="com.agritrace.edairy.service.ui.views.ServiceRequestLogView"
            id="com.agritrace.edairy.service.ui.views.ServiceRequestLogView"
            name="ServiceRequestLogView"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="com.agritrace.edairy.milkCollection.ui.views.MilkCollectionJournalView"
            id="com.agritrace.edairy.milkCollection.ui.views.MilkCollectionJournalView"
            name="MilkCollectionJournalView">
      </view>
          <view
            allowMultiple="true"
            class="com.agritrace.edairy.ui.views.DeliveryView"
            id="com.agritrace.edairy.ui.views.DeliveryView"
            name="DeliveryView">
        </view>
        <view
            allowMultiple="true"
            class="com.agritrace.edairy.finance.ui.CreditJournalView"
            id="com.agritrace.edairy.finance.ui.CreditJournalView"
            name="CreditJournalViews">
        </view>
         <view
            allowMultiple="true"
            class="com.agritrace.edairy.ui.views.NetworklDairyView"
            id="com.agritrace.edairy.ui.views.NetworklDairyView"
            name="NetworklDairyView">
        </view>
         <view
               allowMultiple="true"
               class="com.agritrace.edairy.ui.views.BlankView"
               id="com.agritrace.edairy.ui.views.BlankView"
               name="Blank"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               class="com.agritrace.edairy.ui.views.MilkProductionReportView"
               id="com.agritrace.edairy.ui.views.MilkProductionReportView"
               name="Milk Production Report"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               class="com.agritrace.edairy.member.ui.views.MemberSearchView"
               id="com.agritrace.edairy.member.ui.views.MemberSearchView"
               name="Member Search"
               restorable="true">
         </view>
        
         <view
               allowMultiple="true"
               class="com.agritrace.edairy.ui.views.MemberPayablesReportView"
               id="com.agritrace.edairy.ui.views.MemberPayablesReportView"
               name="Member Payables Report"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               class="com.agritrace.edairy.ui.views.MonthlyCreditReportView"
               id="com.agritrace.edairy.ui.views.MonthlyCreditReportView"
               name="Monthly Credit Report"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               class="com.agritrace.edairy.ui.views.MemberStatementReportView"
               id="com.agritrace.edairy.ui.views.MemberStatementReportView"
               name="Member Statement Report"
               restorable="true">
         </view>
        </extension>
   <extension
         point="org.eclipse.riena.ui.swt.imagePaths">
      <path path="resources" />
      <path path="icons" />
   </extension>
   <extension
         id="dairy_manager"
         name="Dairy Manager"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="com.agritrace.edairy.ui.EDairyManagerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="edairy_demo_product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.agritrace.edairy.demo.riena.dairy_manager"
            name="eDairy Desktop Manager">
         <property
               name="appName"
               value="eDairy Desktop Manager">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         id="edairy_manager"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.agritrace.edairy.demo.riena.dairy_manager"
            name="eDairy Manager">
         <property
               name="appName"
               value="eDairy Manager">
         </property>
         <property
               name="aboutImage"
               value="resources/edairy_helpabout.png">
         </property>
         <property
               name="aboutText"
               value="eDairy Manager v0.1a">
         </property>
         <property
               name="windowImages"
               value="icons/cow16.png,icons/cow32.png,icons/cow48.png,icons/cow64.png,icons/cow128.png">
         </property>
      </product>
   </extension>
   <extension
         id="newId"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="com.agritrace.edairy.ui.EDairyManagerApplication">
         </run>
      </application>
   </extension>
</plugin>
